    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-root-import": "^4.1.3",
    "babel-runtime": "^6.11.6",
    "bootstrap-sass": "^3.3.7",
