    "sass-loader": "^6.0.3",
    "scss": "^0.2.4",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-livereload-plugin": "^0.11.0"
